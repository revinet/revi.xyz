{
  "exclude": [
    "(^.gitignore$)",
    "(^docusaurus/build/)",
    "(^docusaurus/package-lock.json$)",
    "(^node_modules/)",
    "(^package-lock.json$)"
  ],
  "linters": {
    "chmod": {
      "type": "chmod"
    },
    "css": {
      "type": "csslint",
      "include": [
        "(\\.css$)"
      ]
    },
    "Generated": {
      "type": "generated"
    },
    "json": {
      "type": "json",
      "include": [
        "(\\.arclint$)",
        "(\\.arcunit$)",
        "(\\.eslintrc$)",
        "(\\.imgbotconfig$)",
        "(\\.json$)",
        "(^\\.arcconfig$)"
      ]
    },
    "merge-conflict": {
      "type": "merge-conflict"
    },
    "nolint": {
      "type": "nolint"
    },
    "spelling": {
      "type": "spelling"
    },
    "text": {
      "type": "text",
      "text.max-line-length": 80,
      "severity": {
        "1": "advice",
        "2": "advice",
        "3": "advice",
        "5": "disabled"
      }
    }
  }
}
